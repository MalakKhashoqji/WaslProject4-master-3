<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data/>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/eventitem"
        android:layout_width="match_parent"
        android:layout_height="@dimen/item1_view_holder_eventitem_height"
        android:background="@color/item1_view_holder_eventitem_background_color">
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/eventdateandimg_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/item1_view_holder_eventdateandimg_constraint_layout_height"
            android:layout_marginLeft="@dimen/item1_view_holder_eventdateandimg_constraint_layout_margin_start"
            android:layout_marginRight="@dimen/item1_view_holder_eventdateandimg_constraint_layout_margin_end"
            android:layout_marginTop="@dimen/item1_view_holder_eventdateandimg_constraint_layout_margin_top"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/eventprovider_constraint_layout"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="98dp">

            <ImageView
                android:id="@+id/eventimg_image_view"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/item1_view_holder_eventimg_image_view_margin_start"
                android:scaleType="centerCrop"
                android:src="@drawable/mask_12"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="17dp"
                tools:layout_editor_absoluteY="0dp"/>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/eventnameloc_constraint_layout"
                android:layout_width="@dimen/item1_view_holder_eventnameloc_constraint_layout_width"
                android:layout_height="@dimen/item1_view_holder_eventnameloc_constraint_layout_height"
                android:layout_marginRight="@dimen/item1_view_holder_eventnameloc_constraint_layout_margin_end"
                android:layout_marginTop="@dimen/item1_view_holder_eventnameloc_constraint_layout_margin_top"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="210dp"
                tools:layout_editor_absoluteY="60dp">

                <TextView
                    android:id="@+id/eventname_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/item1_view_holder_eventname_text_view_margin_start"
                    android:fontFamily="@font/font_applesystemuifont"
                    android:gravity="end"
                    android:letterSpacing="0.03"
                    android:text="@string/item1_view_holder_eventname_text_view_text"
                    android:textColor="@color/item1_view_holder_eventname_text_view_text_color"
                    android:textSize="@dimen/item1_view_holder_eventname_text_view_text_size"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="21dp"
                    tools:layout_editor_absoluteY="0dp"/>

                <TextView
                    android:id="@+id/eventloc_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/item1_view_holder_eventloc_text_view_margin_bottom"
                    android:layout_marginLeft="@dimen/item1_view_holder_eventloc_text_view_margin_start"
                    android:fontFamily="@font/font_applesystemuifont"
                    android:gravity="end"
                    android:letterSpacing="0.03"
                    android:text="@string/item1_view_holder_eventloc_text_view_text"
                    android:textColor="@color/item1_view_holder_eventloc_text_view_text_color"
                    android:textSize="@dimen/item1_view_holder_eventloc_text_view_text_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    tools:layout_editor_absoluteX="2dp"
                    tools:layout_editor_absoluteY="23dp"/>
            </android.support.constraint.ConstraintLayout>
        
            <android.support.constraint.ConstraintLayout
                android:id="@+id/eventdate_constraint_layout"
                android:layout_width="@dimen/item1_view_holder_eventdate_constraint_layout_width"
                android:layout_height="@dimen/item1_view_holder_eventdate_constraint_layout_height"
                android:layout_marginTop="@dimen/item1_view_holder_eventdate_constraint_layout_margin_top"
                android:background="@drawable/item1_view_holder_eventdate_constraint_layout_background"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="16dp">

                <TextView
                    android:id="@+id/eventdate_text_view"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/item1_view_holder_eventdate_text_view_margin_start"
                    android:layout_marginRight="@dimen/item1_view_holder_eventdate_text_view_margin_end"
                    android:fontFamily="@font/font_roboto_medium"
                    android:gravity="center"
                    android:letterSpacing="0.03"
                    android:textColor="@color/item1_view_holder_eventdate_text_view_text_color"
                    android:textSize="@dimen/item1_view_holder_eventdate_text_view_text_size"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="5dp"
                    tools:layout_editor_absoluteY="7dp"
                    tools:text="@string/item1_view_holder_eventdate_text_view_text"/>
            </android.support.constraint.ConstraintLayout>
        </android.support.constraint.ConstraintLayout>

        <TextView
            android:id="@+id/eventdesc_text_view"
            android:layout_width="@dimen/item1_view_holder_eventdesc_text_view_width"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/item1_view_holder_eventdesc_text_view_margin_bottom"
            android:layout_marginLeft="@dimen/item1_view_holder_eventdesc_text_view_margin_start"
            android:layout_marginRight="@dimen/item1_view_holder_eventdesc_text_view_margin_end"
            android:fontFamily="@font/font_roboto_light"
            android:gravity="start"
            android:lineSpacingMultiplier="1.26"
            android:text="@string/item1_view_holder_eventdesc_text_view_text"
            android:textColor="@color/item1_view_holder_eventdesc_text_view_text_color"
            android:textSize="@dimen/item1_view_holder_eventdesc_text_view_text_size"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="255dp"/>
    
        <android.support.constraint.ConstraintLayout
            android:id="@+id/eventprovider_constraint_layout"
            android:layout_width="0dp"
            android:layout_height="@dimen/item1_view_holder_eventprovider_constraint_layout_height"
            android:layout_marginLeft="@dimen/item1_view_holder_eventprovider_constraint_layout_margin_start"
            android:layout_marginRight="@dimen/item1_view_holder_eventprovider_constraint_layout_margin_end"
            android:layout_marginTop="@dimen/item1_view_holder_eventprovider_constraint_layout_margin_top"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="31dp"
            tools:layout_editor_absoluteY="20dp">
        
            <TextView
                android:id="@+id/providername_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/item1_view_holder_providername_text_view_margin_start"
                android:layout_marginTop="@dimen/item1_view_holder_providername_text_view_margin_top"
                android:fontFamily="@font/font_roboto_light"
                android:gravity="start"
                android:text="@string/item1_view_holder_providername_text_view_text"
                android:textColor="@color/item1_view_holder_providername_text_view_text_color"
                android:textSize="@dimen/item1_view_holder_providername_text_view_text_size"
                app:layout_constraintLeft_toRightOf="@+id/userimg_image_view"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="6dp"/>
        
            <ImageView
                android:id="@+id/moretap_image_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="center"
                android:src="@drawable/combined_shape"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="296dp"
                tools:layout_editor_absoluteY="12dp"/>
        
            <ImageView
                android:id="@+id/userimg_image_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="center"
                android:src="@drawable/layer_1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="0dp"
                tools:layout_editor_absoluteY="0dp"/>
        
            <TextView
                android:id="@+id/eventposttime_text_view"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/item1_view_holder_eventposttime_text_view_margin_start"
                android:layout_marginTop="@dimen/item1_view_holder_eventposttime_text_view_margin_top"
                android:fontFamily="@font/font_roboto_light"
                android:gravity="start"
                android:text="@string/item1_view_holder_eventposttime_text_view_text"
                android:textColor="@color/item1_view_holder_eventposttime_text_view_text_color"
                android:textSize="@dimen/item1_view_holder_eventposttime_text_view_text_size"
                app:layout_constraintLeft_toRightOf="@+id/userimg_image_view"
                app:layout_constraintTop_toBottomOf="@+id/providername_text_view"
                tools:layout_editor_absoluteX="60dp"
                tools:layout_editor_absoluteY="25dp"/>
        </android.support.constraint.ConstraintLayout>
    </android.support.constraint.ConstraintLayout>
</layout>